# Unicorn
UNICORN_BASE_URL=http://localhost:8001
UNICORN_API_USER=
UNICORN_API_KEY=
# Comma-separated lifecycle states to include (excludes Demised etc.). Example: Registered,Live,Configure
UNICORN_LIFECYCLE_STATES=Registered,Live,Configure

# Illumio
ILLUMIO_PCE_HOST=
ILLUMIO_PCE_PORT=443
ILLUMIO_ORG_ID=1
ILLUMIO_API_KEY=
ILLUMIO_API_SECRET=

# Logging
LOG_LEVEL=INFO

# Cache: background refresh interval (seconds). Default 3600 = 1 hour.
CACHE_REFRESH_INTERVAL_SECONDS=3600
# Delay before first refresh on startup (seconds). Default 5.
CACHE_REFRESH_STARTUP_DELAY_SECONDS=5
